{
  "name": "mockend",
  "version": "0.0.1",
  "description": "An app for mocking backends",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "yarn build",
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:watch": "nodemon",
    "deploy": "git push && git push heroku master",
    "test": "jest --coverage",
    "sql": "node_modules/.bin/sequelize",
    "eslint": "node_modules/.bin/eslint",
    "migrate-db": "node_modules/.bin/sequelize db:migrate && yarn",
    "prod-logs": "heroku logs --tail"
  },
  "keywords": [
    "mock",
    "api",
    "backend"
  ],
  "author": "Kito Clark",
  "license": "ISC",
  "dependencies": {
    "adjective-adjective-animal": "^1.3.3",
    "bcrypt": "^3.0.7",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "passport": "^0.4.1",
    "pg": "^7.14.0",
    "pluralize": "^8.0.0",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.21",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
